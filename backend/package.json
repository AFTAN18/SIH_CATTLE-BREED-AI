{
  "name": "cattle-breed-identification-backend",
  "version": "1.0.0",
  "description": "Backend API for cattle/buffalo breed identification system",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd ../frontend && npm run build",
    "build:server": "echo 'Server build complete'",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "node src/database/migrate.js",
    "db:seed": "node src/database/seed.js",
    "db:reset": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "cattle",
    "breed",
    "identification",
    "ai",
    "ml",
    "agriculture",
    "livestock"
  ],
  "author": "Cattle ID Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bull": "^4.12.0",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "rate-limiter-flexible": "^3.0.8",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "multer-s3": "^3.0.1",
    "aws-sdk": "^2.1490.0",
    "tensorflow": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "canvas": "^2.11.2",
    "jimp": "^0.22.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.1.0",
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
