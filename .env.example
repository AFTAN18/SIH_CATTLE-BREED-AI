# Frontend Environment Variables
VITE_API_URL=http://localhost:3001
VITE_APP_NAME=Bharat Pashudhan - Cattle Breed Identification
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Backend Environment Variables (for reference)
# Copy to backend/.env for backend configuration
DATABASE_URL=postgresql://username:password@localhost:5432/cattle_breed_db
REDIS_URL=redis://localhost:6379
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-in-production
FRONTEND_URL=http://localhost:5173

# File Upload Configuration
UPLOAD_DIR=uploads
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# AI Model Configuration
MODEL_PATH=./models/cattle-breed-model.json
MODEL_WEIGHTS_PATH=./models/cattle-breed-weights.bin
CONFIDENCE_THRESHOLD=0.7

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Security
BCRYPT_ROUNDS=12
CORS_ORIGIN=http://localhost:5173
SESSION_SECRET=your-session-secret-change-in-production

# External Services (Optional)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
ANALYTICS_API_KEY=your-analytics-api-key
NOTIFICATION_SERVICE_KEY=your-notification-service-key
